n = int(input("Nhap so nguyen duong n: "))
reverse = str("")
check = 0

while check == 0:
    tmp = n
    reverse = ""
    while tmp > 0:
        reverse += str(tmp % 10)
        tmp //= 10

    m = 0
    for x in reverse:
        m = m * 10 + int(x)
    n += m

    s = str(n)
    check = 1
    for i in range(0, len(s) // 2):
        if s[i] != s[len(s) - 1 - i]:
            check = 0

print(f'So can tim la {n}')



